{
	"position": "left",
	"layer": "top",
	"modules-left": [
		"hyprland/workspaces"
	],
	"modules-center": [
	],
	"modules-right": [
		"clock"
	],

	"clock": {
		"format": "{:%I:%M\n%p}"
	},

	// center
	"hyprland/workspaces": {
		"format": "{icon}",
		"format-icons": {
			"default": "",
			"active": ""
		},
		"active-only": "false",
		"on-scroll-up": "hyprctl dispatch workspace e-1",
		"on-scroll-down": "hyprctl dispatch workspace e+1"
	}
}
